FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 44.74;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 44.74;
    }
    outlet
    {
        type            zeroGradient;
    }
    open
    {
        type            omegaWallFunction;
        value           uniform 44.74;
    }
    airfoil_wing
    {
        type            omegaWallFunction;
        value           uniform 44.74;
    }
    symmetry
    {
        type            symmetry;
    }
}


