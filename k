FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.445;

boundaryField
{
    inlet
    {
        type            fixedValue;

//        type            turbulentIntensityKineticEnergyInlet;
//        intensity       0.05;           // 5% turbulence
        value           uniform 0.05;
    }

    open
    {
        type            kqRWallFunction;
        value           uniform 1;
    }

    symmetry
    {
        type            symmetry;
    }
    
    outlet
    {
        type            zeroGradient;
    }

    airfoil_wing
    {
        type            kqRWallFunction;
        value           uniform 1;
    }



}

